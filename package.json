    "build": "webpack && electron-builder"
  },
  "build": {
    "asarUnpack": "./resources/vbs",
    "appId": "com.example.electron-react",
    "files": [
      "package.json",
      "src/index.html",
      "src/electron.js",
      {
        "from": "./build/webpack",
        "to": "src"
      },
      {
        "from": "./resources/vbs",
        "to": "src/vbs"
      }
    ],
    "productName": "Satelli",
    "directories": {
      "buildResources": "resources",
      "output": "build/release"
    },
    "nsis": {
      "perMachine": true,
      "uninstallDisplayName": "Satelli"
    },
    "publish": null
  }
